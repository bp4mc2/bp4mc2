dct:type tax:Activiteit;	 	
  kddef:status kdstat:Gevalideerd;
.
tax:Ontgrondingsactiviteiten a skos:Concept, prov:Entity;
skos:prefLabel "Ontgrondingsactiviteiten";
rdfs:label "Ontgrondingsactiviteiten";
