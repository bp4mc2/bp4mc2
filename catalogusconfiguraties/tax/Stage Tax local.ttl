			GRAPH ?g {
				?concept ?p ?o .
				?concept kddef:status ?status.
				FILTER NOT EXISTS {
					?concept prov:invalidatedAtTime ?time.
				}
				FILTER (
					?status = kdstat:Gevalideerd ||
					?status = kdstat:Geverifieerd
				)
				FILTER NOT EXISTS {
					{
						?o kddef:status kdstat:Afgekeurd.
