# TODO: Overige TopicCategories opnemen

#
# Part D.3
#
inspire-collection:SpatialDataServiceTypes a skos:Collection;
  dct:source inspire-lex:PartD3;
  rdfs:label "Spatial data service types"@en;
  skos:member inspire-concept:DiscoveryService;
  skos:member inspire-concept:ViewService;
  skos:member inspire-concept:DownloadService;
  skos:member inspire-concept:TransformationService;
  skos:member inspire-concept:InvokeSpatialDataService;
  skos:member inspire-concept:OtherService;
.
inspire-concept:DiscoveryService a skos:Concept;
  dct-source inspire-lex:PartD3-1;
  rdfs:label "Discovery service"@en;
  rdfs:label "Zoekdienst"@nl;
  skos:definition "Services making it possible to search for spatial data sets and services on the basis of the content of the corresponding metadata and to display the content of the metadata."@en;
  skos:notation "discovery";
.
inspire-concept:ViewService a skos:Concept;
  dct-source inspire-lex:PartD3-2;
  rdfs:label "View service"@en;
  rdfs:label "Raadpleegdienst"@nl;
  skos:definition "Service that makes it possible, as a minimum, to display, navigate, zoom in and out, pan or overlay viewable spatial data sets and to display legend information and any relevant content of metadata."@en;
  skos:notation "view";
.
inspire-concept:DownloadService a skos:Concept;
  dct-source inspire-lex:PartD3-3;
  rdfs:label "Download service"@en;
  rdfs:label "Downloaddienst"@nl;
  skos:definition "Service that enables copies of spatial data sets, or parts of such sets, to be downloaded and, where practicable, accessed directly."@en;
  skos:notation "download";
.
inspire-concept:TransformationService a skos:Concept;
  dct-source inspire-lex:PartD3-4;
  rdfs:label "Transformation service"@en;
  rdfs:label "Verwerkingsdienst"@nl;
  skos:definition "Service that enables spatial data sets to be transformed with a view to achieving interoperability."@en;
  skos:notation "transformation";
.
inspire-concept:InvokeSpatialDataService a skos:Concept;
  dct-source inspire-lex:PartD3-5;
  rdfs:label "Invoke spatial data service"@en;
  rdfs:label "Oproepdienst"@nl;
  skos:definition "Service that allows defining both the data inputs and data outputs expected by the spatial service and a workflow or service chain combining multiple services. It also allows defining the external web service interface of the workflow or service chain."@en;
  skos:notation "invoke";
.
inspire-concept:OtherService a skos:Concept;
  dct-source inspire-lex:PartD3-6;
  rdfs:label "Other service"@en;
  rdfs:label "Andere dienst"@nl;
  skos:definition ""@en;
  skos:notation "other";
.

#
# Part D.4
#
inspire-collection:ClassificationOfSpatialDataServices a skos:Collection;
  rdfs:label "Classification of spatial data services"@en;
  skos:definition "The keywords are based on the geographic services taxonomy of EN ISO 19119. This taxonomy is organised in categories, the subcategories defining the value domain of the classification of spatial data services."@en;
  skos:member inspire-concept:GeographicHumanInteractionServices;
  skos:member inspire-concept:CatalogueViewer;
  skos:member inspire-concept:GeographicViewer;
  skos:member inspire-concept:GeographicSpreadsheetViewer;
  skos:member inspire-concept:ServiceEditor;
  #TODO Compleet maken
.
inspire-concept:GeographicHumanInteractionServices a skos:Concept;
  rdfs:label "Geographic human interaction services"@en;
  skos:notation "humanInteractionService";
.
inspire-concept:CatalogueViewer a skos:Concept;
  rdfs:label "Catalogue viewer"@en;
  skos:definition "Client service that allows a user to interact with a catalogue to locate, browse, and manage metadata about geographic data or geographic services."@en;
  skos:notation "humanCatalogueViewer";
  skos:broader inspire-concept:GeographicHumanInteractionServices;
.

#
# Part D.5
#
inspire-collection:DegreeOfComformity a skos:Collection;
  rdfs:label "Degree of comformity"@en;
  skos:member inspire-concept:Conformant;
  skos:member inspire-concept:NotConformant;
  skos:member inspire-concept:NotEvaluated;
.
inspire-concept:Conformant a skos:Concept;
  rdfs:label "Conformant"@en;
  skos:definition "The resource is fully conformant with the cited specification."@en;
  skos:notation "conformant";
.
inspire-concept:NotConformant a skos:Concept;
  rdfs:label "Not conformant"@en;
  skos:definition "The resource does not conform to the cited specification."@en;
  skos:notation "notConformant";
.
inspire-concept:NotEvaluated a skos:Concept;
  rdfs:label "Not evaluated"@en;
  skos:definition "Conformance has not been evaluated."@en;
  skos:notation "notEvaluated";
.

#
# Artikel 2 definities
#
inspire-concept:CodeList a skos:Concept;
  dct:source inspire-lex:Artikel2-5;
  rdfs:label "Code list"@en;
  rdfs:label "Codelijst"@nl;
  skos:definition "an open enumeration that can be extended";
.
inspire-concept:Enumeration a skos:Concept;
  dct:source inspire-lex:Artikel2-7;
  rdfs:label "Enumeration"@en;
  rdfs:label "Opsomming"@nl;
  skos:definition "a data type whose instances form a fixed list of named literal values. Attributes of an enumerated type may only take values from this list";
.
inspire-concept:EndPoint a skos:Concept;
  dct:source inspire-lex:Artikel2-31;
  rdfs:label "End point"@en;
  rdfs:label "Eindpunt"@nl;
  skos:definition "the internet address used to directly call an operation provided by a spatial data service";
.
inspire-concept:AccessPoint a skos:Concept;
  dct:source inspire-lex:Artikel2-32;
  rdfs:label "Access point"@en;
  rdfs:label "Toegangspunt"@nl;
  skos:definition "an internet address containing a detailed description of a spatial data service, including a list of end points to allow its execution";
.
#
# Artikel 13
#
inspire-concept:SpatialDatasetCoordinateReferenceSystem a skos:Concept;
  dct:source inspire-lex:Artikel13-1;
  rdfs:label "Spatial data set Coordinate Reference System"@en;
  rdfs:label "Coördinaatreferentiesysteem van de verzameling ruimtelijke gegevens"@nl;
  skos:definition "Description of the coordinate reference system(s) used in the data set."@en;
  skos:definition "Beschrijving van het (de) in de gegevensverzameling gebruikte coördinaatreferentiesyste(e)m(en)."@nl;
.
inspire-concept:SpatialDatasetTemporalReferenceSystem a skos:Concept;
  dct:source inspire-lex:Artikel13-2;
  rdfs:label "Spatial data set Temporal Reference System"@en;
  rdfs:label "Tijdsreferentiesysteem van de verzameling ruimtelijke gegevens"@nl;
  skos:definition "Description of the temporal reference system(s) used in the data set."@en;
