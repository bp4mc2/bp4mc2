inspire-shape:FreeText a sh:Nodeshape;
  sh:name "Free text";
.

#
# Waardelijsten
#
inspire-shape:ResourceTypes a sh:NodeShape;
  sh:name "CodeList (see annex B.5.25 of ISO 19115)";
  sh:property [
    sh:inversePath [sh:path skos:member];
    sh:hasValue inspire-collection:ResourceTypes
  ]
.
inspire-shape:TopicCategories a sh:NodeShape;
  sh:name "Enumeration (See B.5.27 of ISO 19115 or Part D 2 of [Regulation 1205/2008])";
  sh:property [
    sh:inversePath [sh:path skos:member];
    sh:hasValue inspire-collection:TopicCategories
  ]
.
inspire-shape:SpatialDataServiceTypes a sh:NodeShape;
  sh:name "Code list. See Part D 3 of [Regulation 1205/2008]";
  sh:property [
    sh:inversePath [sh:path skos:member];
    sh:hasValue inspire-collection:SpatialDataServiceTypes
  ]
.
inspire-shape:AccessAndUseConditions a sh:NodeShape;
  sh:name "Codelist (strictly limited to the value defined in B.5.24 of ISO 19115)";
  sh:property [
    sh:inversePath [sh:path skos:member];
    sh:hasValue inspire-collection:AccessAndUseConditions
  ]
.
inspire-shape:PublicAccessLimitations a sh:NodeShape;
  sh:name "Anex D.1 Limitations on public access";
  sh:property [
    sh:inversePath [sh:path skos:member];
    sh:hasValue inspire-collection:PublicAccessLimitations
  ]
.
inspire-shape:ResponsiblePartyRoles a sh:NodeShape;
  sh:name "Codelist (see B.5.5 of ISO 19115)";
  sh:property [
    sh:inversePath [sh:path skos:member];
    sh:hasValue inspire-collection:ResponsiblePartyRoles
  ]
.

#
# Technische waardelijsten
#
inspire-collection:PublicAccessLimitations a skos:Collection;
  rdfs:label "Limitations on public access";
  skos:member inspire-concept:NoLimitations;
  skos:member inspire-concept:INSPIRE_Directive_Article13_1a;
  skos:member inspire-concept:INSPIRE_Directive_Article13_1b;
  skos:member inspire-concept:INSPIRE_Directive_Article13_1c;
  skos:member inspire-concept:INSPIRE_Directive_Article13_1d;
  skos:member inspire-concept:INSPIRE_Directive_Article13_1e;
  skos:member inspire-concept:INSPIRE_Directive_Article13_1f;
  skos:member inspire-concept:INSPIRE_Directive_Article13_1g;
  skos:member inspire-concept:INSPIRE_Directive_Article13_1h;
.
